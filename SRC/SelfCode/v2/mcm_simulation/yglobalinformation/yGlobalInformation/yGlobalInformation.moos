//告知所有进程MOOSDB的连接方式，默认是localhost:9000
ServerPort = 9000
Serverhost = localhost

ProcessConfig = Allocation
{
    //CMOOSApp调用iterate的频率
    AppTick = 10 
    //CommsClient调用，决定app与MOOSDB通信频率
    CommsTick = 10 
    VehicleName = younghz_Vehicle
    InitialLocation = [3x1]{0, 1, 2}
    InitialConditions = Bilge = off, BatteryVoltage = 101, Heading = 0.56
}
